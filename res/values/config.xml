<?xml version="1.0" encoding="utf-8"?>
<resources>

	<!-- Output some debug strings -->
	<item name="debug_output" type="bool">true</item>

	<!-- Property to read for ROM version -->
	<string name="property_version">ro.pixys.display.version</string>

	<!-- Property to read for device identifier -->
	<string name="property_device">ro.pixys.device</string>

	<!-- %s is expanded to property_version value. .zip, .delta, .update, .sign extensions added when needed -->
	<string name="filename_base">%s</string>

	<!-- Path deltas are stored, relative to sdcard root -->
	<string name="path_base">OpenDelta</string>

	<!-- the branch name to format URLs with -->
	<string name="url_branch_name">fourteen</string>

	<!-- URL base to fetch all builds. first %s is replaced by branch name later on by property_device -->
	<string name="url_base_json" formatted="false">https://api.pixysos.com/ota/v2/%s/&s/full_update_%s.json</string>

	<!--  URL base to fetch test builds. first %s is replaced by branch name later on by property_device -->
	<string name="test_url_base_json" formatted="false">https://raw.githubusercontent.com/Gabeldorsche/ota/%s/%s/full_update_%s.json</string>

	<!-- Whether device supports A/B performance flashing -->
	<item name="support_ab_perf_mode" type="bool">false</item>

	<!-- Applies to A-only devices only, should be overlayed from DT -->
	<item name="use_twrp" type="bool">false</item>

	<string name="pixys_version">ro.modversion</string>

	<!-- ziptype -->
	<string name="property_ziptype">ro.pixys.edition</string>

	<!-- testmode -->
	<string name="property_test_mode">ro.pixys.ota.test_mode</string>

</resources>
